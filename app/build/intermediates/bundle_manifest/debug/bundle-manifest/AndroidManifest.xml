<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deoxys"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="29" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.deoxys.ActivityMainMenuDeoxys"
            android:screenOrientation="sensorLandscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.deoxys.ActivityMercury"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityVenus"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityEarth"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityMars"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityJupiter"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivitySaturn"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityUranus"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityNeptune"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivitySun"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityTrivia"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityTriviaResult"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name="com.example.deoxys.ActivityPlanetarium"
            android:screenOrientation="sensorLandscape" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>